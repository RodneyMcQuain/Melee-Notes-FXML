<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="250.0" prefWidth="300.0" stylesheets="@../assets/stylesheets/darkStylesheet.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <VBox prefHeight="54.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <fx:include source="header.fxml"/>
            <Label fx:id="lblSpecifiedGame" alignment="CENTER" prefHeight="53.0" prefWidth="272.0" text="Specified Game">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </top>
   <center>
      <GridPane prefHeight="150.0" prefWidth="206.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="114.0" minWidth="10.0" prefWidth="114.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="50.0" prefWidth="50.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Outcome:" />
            <Label text="My Character:" GridPane.rowIndex="1" />
            <Label text="Opponent Character:" GridPane.rowIndex="2" />
            <Label text="Stage:" GridPane.rowIndex="3" />
            <ComboBox fx:id="cbOutcome" prefWidth="150.0" GridPane.columnIndex="1" />
            <ComboBox fx:id="cbMyCharacter" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <ComboBox fx:id="cbOpponentCharacter" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <ComboBox fx:id="cbStage" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <HBox prefHeight="30.0" prefWidth="76.0" GridPane.columnSpan="2" GridPane.rowIndex="4">
               <children>
                  <Button fx:id="btApplyChanges" mnemonicParsing="false" onAction="#onClick_btApplyChanges" text="Apply Changes">
                     <HBox.margin>
                        <Insets right="10.0" top="10.0" />
                     </HBox.margin></Button>
                  <Button fx:id="btRemoveGame" mnemonicParsing="false" onAction="#onClick_btRemoveGame" text="Remove Game">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </GridPane>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
